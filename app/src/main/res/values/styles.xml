<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- AppCompatActivity-->
    <style name="MyTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/dimgray</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/customBlue</item>
        <item name="android:windowTranslucentStatus">true</item>
        <!--<item name="android:windowNoTitle">true</item>-->
        <!--<item name="android:windowFullscreen">true</item>-->
        <!--<item name="android:windowActionBar">false</item>-->
    </style>

    <style name="MyTheme_White" parent="MyTheme">
        <item name="theme_name">经典白</item>
        <item name="theme_isLight">true</item>
        <item name="theme_alpha">2</item>
        <item name="mainBackgrount">@drawable/ic_theme_white</item>
        <item name="listBackgrount">@color/listBackgrount</item>
        <item name="listItem">@color/listItem</item>
        <item name="textColor">#333333</item>
        <item name="textSecondaryColor">#666666</item>
        <item name="less_half_transparent_contrast">@color/less_half_transparent_black</item>
        <item name="half_transparent_contrast">@color/half_transparent_black</item>
        <item name="more_half_transparent_contrast">@color/more_half_transparent_black</item>
        <item name="popupwindow_backgound">@color/more_half_transparent_white</item>
    </style>

