<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WaveProgressView">
        <attr name="WPV_radius" format="dimension|reference" />
        <attr name="WPV_radius_color" format="color|reference" />
        <attr name="WPV_progress_text_color" format="color|reference" />
        <attr name="WPV_progress_text_size" format="dimension|reference" />
        <attr name="WPV_progress_color" format="color|reference" />
        <attr name="WPV_progress_color_negate" format="color|reference" />
        <attr name="WPV_progress" format="float" />
        <attr name="WPV_maxProgress" format="float" />
    </declare-styleable>


    <declare-styleable name="myTheme">
        <attr name="theme_name" format="string"/>
        <attr name="theme_isLight" format="boolean"/>
        <attr name="theme_alpha" format="integer"/>
        <attr name="listBackgrount" format="reference"/>
        <attr name="listItem" format="reference"/>
        <attr name="mainBackgrount" format="reference"/>
        <attr name="less_half_transparent_contrast" format="reference"/>
        <attr name="half_transparent_contrast" format="reference"/>
        <attr name="more_half_transparent_contrast" format="reference"/>
        <attr name="textColor" format="color"/>
        <attr name="textSecondaryColor" format="color"/>
        <attr name="popupwindow_backgound" format="reference"/>
    </declare-styleable>

</resources>